if __name__ == '__main__':
    ls =[]
    for _ in range(int(input())):
        name = input()
        score = float(input())
        ls.append([score,name])
        ls.sort()

    for i in range(len(ls)-1):
        if ls[i][0] == ls[i+1][0]:
            num = ls[i+1][0]
        else: 
            num = ls[i+1][0]
            break
    for i in ls:
        if i[0] == num:
            print(i[1])
            
        
        
    
    
    
